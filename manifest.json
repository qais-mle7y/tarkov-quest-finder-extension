{
    "manifest_version": 3,
    "name": "Tarkov Quest Finder",
    "version": "1.0",
    "description": "Search for Escape from Tarkov quests and view their details.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "questFinder.html",
      "default_icon": {
        "16": "icons/escape-from-tarkov-16.png",
        "48": "icons/escape-from-tarkov-48.png",
        "128": "icons/escape-from-tarkov-128.png"
      }
    },
    "icons": {
      "16": "icons/escape-from-tarkov-16.png",
      "48": "icons/escape-from-tarkov-48.png",
      "128": "icons/escape-from-tarkov-128.png"
    }
}